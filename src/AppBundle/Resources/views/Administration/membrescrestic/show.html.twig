                <td>{{ membresCrestic.etage }}</td>
            </tr>
            <tr>
                <th>Bureau</th>
                <td>{{ membresCrestic.bureau }}</td>
            </tr>
            <tr>
                <th>Emailperso</th>
                <td>{{ membresCrestic.emailPerso }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ membresCrestic.adresse }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if membresCrestic.created %}{{ membresCrestic.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if membresCrestic.updated %}{{ membresCrestic.updated|date('Y-m-d H:i:s') }}{% endif %}</td>

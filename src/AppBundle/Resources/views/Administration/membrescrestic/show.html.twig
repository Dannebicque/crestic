                <td>{% if membresCrestic.ancienMembresCrestic %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('administration_membres_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('administration_membres_edit', { 'id': membresCrestic.id }) }}">Edit</a>
        </li>
        <div class="col-sm-3" style="text-align: right" >
            <a href="{{ path('administration_membres_update_ancien') }}" class="btn btn-danger">Marquer comme ancien</a>
        </div>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
